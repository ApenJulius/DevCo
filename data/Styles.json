    "whiteTheme": {
      "texts": {
        "injectionText": "%cInjection%c - %c%s at%c %s%c",
        "devModeText": "%cDevMode%c - %c%s",
        "listenerModeText": "%cListener%c - %c%s"
      },
      "colors": {
        "bg": "transparent",
        "textColor": "#fff",

        "injectionBg": "#009AFA",
