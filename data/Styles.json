                "warningBg": "#FFA500",
                "warningForeground": "#FFD966",
                "warningText": "#000000",

                "injectionBg": "#009AFA",
                "injectionForeground": "#66C4FF",
                "injectionText": "#000000",

                "devModeBg": "#FF4DCF",
                "devModeForeground": "#FFA3E7",
                "devModeText": "#000000",
                "devModeSecondaryText": "#717880",

                "listenerModeBg": "#FFF700",
                "listenerModeForeground": "#FFFB7A",
                "listenerModeText": "#000000",
                "listenerModeSecondaryText": "#717880",

                "errorBg": "#FF0854",
                "errorText": "#000000",
                "errorPadding": "4px",
                "errorBorderRadius": "4px",
                "errorFontWeight": "bold"
            }
