{
    "version": "0.0.1",
    "defaultTheme": "whiteTheme",
    "themes": {
        "whiteTheme": {
            "texts": {
                "debugText": "%cDebug%c - %c%s",
                "infoText": "%cInfo%c - %c%s",
                "noticeText": "%cNotice%c - %c%s",
                "warningText": "%cWarning%c - %c%s",
                "errorText": "%cError%c - %c%s",
                "criticalText": "%cCritical%c - %c%s",
                "alertText": "%cAlert%c - %c%s",
                "emergencyText": "%cEmergency%c - %c%s",
                "securityText": "%cSecurity%c - %c%s",
                "performanceText": "%cPerformance%c - %c%s",
                "auditText": "%cAudit%c - %c%s",
                "verboseText": "%cVerbose%c - %c%s",
                "debug2Text": "%cDebug2%c - %c%s",
                "debug3Text": "%cDebug3%c - %c%s",

                "injectionText": "%cInjection%c - %cInjected %s at %s",
                "devModeText": "%cDevMode%c - %c%s",
                "listenerModeText": "%cListener%c - %c%s"
            },
            "colors": {
                "bg": "transparent",
                "textColor": "#fff",

                "bubble": "padding: 4px; border-radius: 4px; font-weight: bold;",
